#tag WindowCode
	#tag Method, Flags = &h0
		Sub RefreshTime()
		  dim curweather as OpenWeatherData = OpenWeather.GetCurrentWeather(LocationID, API_ID, _
		  "units=metric", "lang=de")
		  if curweather <> nil then
		    TemperatureLabel.text = curweather.Temperature.ToText(xojo.core.locale.Current, "##.##")+" Â°C"
		    me.Title = curweather.CityName
		    TimeLabel.Text = curweather.DataTime.ToText(xojo.core.locale.Current)
		  end if
		End Sub
	#tag EndMethod


#tag EndWindowCode

#tag Events Timer1
	#tag Event
		Sub Action()
		  RefreshTime
		End Sub
	#tag EndEvent
#tag EndEvents
